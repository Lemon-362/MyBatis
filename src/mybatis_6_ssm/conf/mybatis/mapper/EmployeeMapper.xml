<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mybatis_5_cache.com.Lemon.mybatis.dao.EmployeeMapper">

    <select id="getEmpById" resultType="mybatis_6_ssm.com.Lemon.mybatis.bean.Employee">
            select * from mybatis.tbl_employee where id = #{id}
    </select>

    <select id="getEmps" resultType="mybatis_6_ssm.com.Lemon.mybatis.bean.Employee">
        select * from mybatis.tbl_employee
    </select>

</mapper>
